<Window x:Class="Hemuppgift_WPFtemplate.DockButton"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="DockButton" Height="450" Width="800">
    <DockPanel>
        <Grid>
        <Button x:Name="LeftButton" Content="Left" Height="30" Width="80" DockPanel.Dock="Left" HorizontalAlignment="Left"/>
        <Button x:Name="RightButton" Content="Right"  Height="30" Width="80" DockPanel.Dock="Right" HorizontalAlignment="Right"/>
        <Button x:Name="TopButton" Content="Top"  Height="30" Width="80" DockPanel.Dock="Top" VerticalAlignment="Top"/>
        <Button x:Name="BottomButton" Content="Bottom" Height="30" Width="80" DockPanel.Dock="Bottom" VerticalAlignment="Bottom" />
        <Button x:Name="CenterButton" Content="Center" Height="30" Width="80" VerticalAlignment="Center" Click="CenterButton_Click" HorizontalAlignment="Center"/>
        <Image x:Name="TrophyImage" Height="300" Width="400"/>
        </Grid>

    </DockPanel>
</Window>
